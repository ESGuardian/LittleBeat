{
  "order": 0,
  "template": "win-proc-list",
  "settings": {
    "index.refresh_interval": "60s",
    "number_of_shards" : 1,
    "number_of_replicas" : 0
  },
  "mappings": {
    "win-proc-list": {
      "properties": {        
        "proc_name": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "severity": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "comment": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "@timestamp": {
          "type": "date"
        },
        "@version": {
          "type": "string"
        }
      }
    }
  }
}

